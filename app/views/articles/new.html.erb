<h1>CREATE</h1>
<% if @article.errors.any? %>
  <% @article.errors.full_messages.each do |msg| %>
    <li><%= msg%> </li>
  <% end%>
<%end%>

<%= form_for @article do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :description %>
  <%= f.text_area :description %>
  <%= f.submit %>
<% end%>